// one-time junk
var MessengerAttentionWidgetFactory = (function () {
    var _factoryInterface = {
        createInstance: function (options) { return null; },
        getFactoryData: function () { if (!_factoryData.isInitialized) { init(); } return _factoryData; },
    };

    // initialize factory/one-time data.  done only when first instance is created
    function init() {
        _factoryData.defaultInstanceOptions = {
            container: null,
            eventContainer: null,
            token: null,
            
            runtime: null,
            messengerContext: null,

            height: 200,
            showClose: false,
            realtimeHubsBase: null,
            makeContainersTransparent: true, // false when running in test mode
            callbacks: {
                onClose: function () { }
            }
        };

        _factoryData.templateDefinitions = {
            control:                
                '<div class="MessengerAttention">' +
                    '<span class="Bubble"><div class="Spacer"></div>Testing long message here</span>' +
                '</div>'
        };

        // auto doT templates
        $.each(_factoryData.templateDefinitions, function (key, templateDefinition) {
            _factoryData.templateInstances[key] = doT.template(templateDefinition);
        });


        _factoryData.isInitialized = true;
    }


    // wrap all factory/one-time data in  a single object so we only have to punch one hole in interface to instance code to use
    var _factoryData = {
        isInitialized: false,
        templateDefinitions: {},
        templateInstances: {}
    };

    return _factoryInterface;
}());




// per-instance junk
MessengerAttentionWidgetFactory.createInstance = function (options) {
    var _interface = {
        destroy: function() {},
        sync: function(changeType, runtime) {}
    };

    // ---------------

    // will trigger factory init() if needed
    var _factoryData = this.getFactoryData();
    var _options = $.extend(true, {}, _factoryData.defaultInstanceOptions, options);    

    var _lastFrameStyles = '';

    var _launcherClicked = false;
    var _timeoutId = null;
    
    function ready() {
        // send a message once frame is loaded and ready to work.  init() gets called afterwards
        window.addEventListener('message', handleEvent, false);

        sendEvent({
            type: 'FRAME_READY',
            frameType: 'Attention'
        });
    }


    function init() {                        
        if (_options.eventContainer == null) _options.eventContainer = _options.container;

        // make all parent elements have transparent background 
        if (_options.makeContainersTransparent) {
            $(_options.container).parents().css('background', 'transparent');
        }

        // initialize view state if it doesnt exist
        _options.context = { viewState: null };
        if (_options.context.viewState == null) {
            _options.context.viewState = {
                view: _options.viewOptions.defaultView,
                launcherButton: {
                    width: 75,
                    height: 75,
                    marginTop: 20
                },
                panel: {
                    width: 300,
                    height: 500
                }
            };
        }

        MessengerRuntimeUtilities.syncStyles(_options.runtime.messenger, getTotalWidth(), getTotalHeight());

        draw();
    }

    _interface.destroy = function () {
        if (_timeoutId != null) {
            clearTimeout(_timeoutId);
        }
    };


    _interface.sync = function(changeType, state) {
        _options.runtime = state.runtime;
        _options.messengerContext = state.messengerContext;        
        styleFrame(_lastFrameStyles);
        
        switch (changeType) {
            case 'settings':          
                MessengerRuntimeUtilities.syncStyles(_options.runtime.messenger, getTotalWidth(), getTotalHeight());
                break;
        }
        
    }


    function styleFrame(styles) {        
        var messengerSettings = _options.runtime.messenger.settings;

        if (messengerSettings.general.messengerPosition == 'right') {
            if ($(_options.container).find('.Bubble').hasClass('LauncherLeft')) {
                $(_options.container).find('.Bubble').removeClass('LauncherLeft');
            }
        }

        if (messengerSettings.general.messengerPosition == 'left') {
            if (!$(_options.container).find('.Bubble').hasClass('LauncherLeft')) {
                $(_options.container).find('.Bubble').addClass('LauncherLeft');
            }
        }

        var horizontalOffset = 37;
        var verticalOffset = 20;

        var baseStyles = 'position: fixed;';
        if (messengerSettings.general.messengerPosition == "right") baseStyles += ' right: ' + (messengerSettings.dimensions.horizontalOffset + horizontalOffset) + 'px;';
        if (messengerSettings.general.messengerPosition == "left") baseStyles += ' left: ' + (messengerSettings.dimensions.horizontalOffset + horizontalOffset) + 'px;';
        baseStyles += ' bottom: ' + (messengerSettings.dimensions.verticalOffset + verticalOffset) + 'px;';

        sendEvent({
            type: 'STYLE_FRAME_CONTAINER',
            frameType: 'Attention',
            styles: baseStyles + styles
        });

        _lastFrameStyles = styles;
    }

    function handleEvent(message) {
        if (message.data == null) return;
        
        var event = message.data;        
        if (event.type == null) return;

        
        switch (event.type) {
            case 'ALL_FRAMES_READY':
                _options.viewOptions = event.settings.viewOptions;
                init();
                break;

            case 'SYNC':
                _interface.sync(event.changeType, event.state);
                break;

            case 'SET_PANEL_VISIBILITY':          
                if (event.visible) {
                    _launcherClicked = true;

                    if (_timeoutId != null) {
                        clearTimeout(_timeoutId);
                    }

                    styleFrame('visibility: visible;  width: ' + 0 + 'px; height: ' + 0 + 'px; overflow: hidden; ');
                }                                
                break;
        }
    }


    function sendEvent(message) {
        window.parent.postMessage(message, '*');
    }

    function draw() {
        _options.totalHeight = getTotalHeight();
        _options.totalWidth = getTotalWidth();
        $(_options.container).html(_factoryData.templateInstances.control(_options));

        $(_options.container).find('.Bubble').html(_options.runtime.messenger.settings.launcher.attentionMessage);                

        styleFrame('visibility: hidden;  width: ' + 1920 + 'px; height: ' + getTotalHeight() + 'px; '); // arbitrarily big width so we can get the real width

        // we have to wait for the style to be applied before we can get accurate height/width calculation
        var msStyleDelay = 100;
        setTimeout(function () {
            var width = getTotalWidth();
            var height = getTotalHeight();

            styleFrame('visibility: visible;  width: ' + 0 + 'px; height: ' + height + 'px; overflow: hidden; ');

            var msUntilShown = Math.max((_options.runtime.messenger.settings.launcher.attentionSeconds * 1000) - msStyleDelay, msStyleDelay);
            if (!_launcherClicked && _timeoutId == null) {
                _timeoutId = setTimeout(function () {
                    if (!_launcherClicked) {
                        sendEvent({
                            type: 'ATTENTION_STARTED',
                        });

                        styleFrame('visibility: visible;  width: ' + width + 'px; height: ' + height + 'px; overflow: hidden; transition: 1s;');
                    }
                }, msUntilShown);
            }
        }, msStyleDelay);
        
    }
    
    function getTotalHeight() {
        var height = $(_options.container).find('.Bubble').height();
        var padding = 11;

        if (height == null || height < 1) {
            return 0;
        }

        return height + padding;
    }


    function getTotalWidth() {
        var width = $(_options.container).find('.Bubble').width();
        var padding = 51;

        if (width == null || width < 1) {
            return 0;
        }

        return width + padding;
    }

    ready();
    return _interface;
};
// one-time junk
var MessengerCanvasWidgetFactory = (function () {
    var _factoryInterface = {
        createInstance: function (options) { return null; },
        getFactoryData: function () { if (!_factoryData.isInitialized) { init(); } return _factoryData; },
    };

    // initialize factory/one-time data.  done only when first instance is created
    function init() {
        _factoryData.defaultInstanceOptions = {
            container: null,         
            eventContainer: null,
            appInstance: null,
            appDefinition: null,
            runType: 'configure',
            callbacks: {
                onUpdated: function(appInstance, callback) {},
                onClosed: function() {},
                onOpenSheet: function (sheet) {},
                onDrawn: function () { },
                onInitialized: function () { },
            }
        };

        _factoryData.templateDefinitions = {            
            canvas:
                '<div class="canvas"></div>',

            div:
                '<div class="canvas_component canvas_div" data-id="{{=it.id}}" data-type="div"></div>',

            text:
                '<div class="canvas_component canvas_text" data-id="{{=it.id}}" data-type="text" data-align="{{=it.align}}">' +
                    '{{?it.style == "header"}}' +
                        '<h4>{{=it.value}}</h4>' +
                    '{{??}}' +
                        // default: paragraph
                        '<p>{{=it.value}}</p>' +
                    '{{?}}' +                    
                '</div>',

            textbox:
               '<div class="canvas_component canvas_textbox_container input-group" data-id="{{=it.id}}" data-type="textbox" {{?it.action != null && it.action.type != null}}data-action="{{=it.action.type}}"{{?}} {{?it.action != null && it.action.url != null}}data-action-url="{{=it.action.url}}"{{?}}>' +
                    '{{?it.prepend != null && it.prepend.length > 0}}' +
                        '<div class="input-group-prepend">' +
                            ' <span class="input-group-text">{{=it.prepend}}</span>' +
                        '</div>' +
                    '{{?}}' +

                    '<input type="text" class="canvas_textbox form-control" {{?it.placeholder != null}}placeholder="{{=it.placeholder}}"{{?}}></input>' +

                    '{{?it.action != null}}' +
                        '<div class="input-group-append">' +
                            '<button type="button" class="btn btn-primary"' +
                            '{{?it.buttonAriaLabel != null && typeof it.buttonAriaLabel != "undefined"}}aria-label="{{=it.buttonAriaLabel}}"{{?}}' +
                            '>></button>' +
                        '</div>' +
                    '{{?}}' +
                '</div>',

            dropdown:
                '<div class="canvas_component canvas_dropdown_container input-group" data-id="{{=it.id}}" data-type="dropdown">' +
                    '{{?it.prepend != null && it.prepend.length > 0}}' +
                        '<div class="input-group-prepend">' +
                            ' <span class="input-group-text">{{=it.prepend}}</span>' +
                        '</div>' +
                    '{{?}}' +

                    '<select class="canvas_component canvas_dropdown custom-select">' +
                        '{{~it.options :option:index}}' + 
                            '<option class="canvas_dropdown_select_option" value="{{=option.id}}">{{=option.label}}</option>' +
                        '{{~}}' +
                    '</select>' +
                '</div>',

            button:
                '<button type="button" class="canvas_button btn btn-flat {{?it.style != null && it.style.length > 0}}btn-{{=it.style}}"{{?}} data-id="{{=it.id}}" data-type="button" {{?it.action != null && it.action.type != null}}data-action="{{=it.action.type}}"{{?}} {{?it.action != null && it.action.url != null}}data-action-url="{{=it.action.url}}"{{?}} ' +
                
                // list of attribute:
                '{{~it.attributes :attribute}} {{=attribute.name}}="{{=attribute.value}}"{{~}}' +

                '>{{=it.label}}</button>',

            list:
                '<div class="canvas_component List" data-id="{{=it.id}}" data-type="list" data-orientation="{{=it.orientation}}">' +

                    '<ul>' +
                    '{{~it.items :item:index}}' +
                        '<li>' + 
                            '<div class="ListItem {{?item.disabled == true}}disabled{{?}}" data-id="{{=item.id}}" {{?item.action != null && item.action.type != null}}data-action="{{=item.action.type}}" tabindex="0"{{?}} {{?item.action != null && item.action.url != null}}data-action-url="{{=item.action.url}}" data-action-url-label="{{=item.title}}"{{?}}>' +
                                
                                // image/icon
                                '{{?item.imageUrl != null || item.icon != null}}' +
                                '<div class="Image" style="width: {{=_u.max(it.items, function(o) { return o.imageWidth; }).imageWidth}}px;">' +
                                    '{{?item.imageUrl != null}}' +
                                        '<img src="{{=item.imageUrl}}" style="width:{{=item.imageWidth}}px; height:{{=item.imageHeight}}px;"><img>' +
                                    '{{?}}' +
                                    '{{?item.icon != null}}' +
                                        '<i class="Icon mi-{{=item.icon}}" title="{{=item.tooltip}}"></i>' +
                                    '{{?}}' +
                                '</div>' +
                                '{{?}}' +

                                '<div class="Info">' +
                                    '{{?item.title != null}}' +
                                        '<div class="Title">{{=item.title}}</div>' +
                                    '{{?}}' +

                                    '{{?item.subtitle != null}}' +
                                        '<div class="Subtitle">{{=item.subtitle}}</div>' +
                                    '{{?}}' +
                                '</div>' +
                            '</div>' +
                        '</li>' +
                    '{{~}}' +
                    '</ul>' +                                           
                '</div>',
            hr:
                '<hr class="canvas_component canvas_hr" data-id="{{=it.id}}" data-type="hr"/>',

            video:
                //'{{=JSON.stringify(it)}}'
                // youtube
                '<div class="Video" data-aspect-ratio="{{=it.aspectRatio}}">' +
                    '{{?it.videoType == "YouTube"}}' +
                        '<iframe width="100%" height="721" src="https://www.youtube.com/embed/{{=it.videoId}}" frameborder="0" allow="accelerometer; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>' +
                    '{{?}}' +
                '</div>',
            spacer:
                '<div class="canvas_component canvas_div {{=it.value}}" data-id="{{=it.id}}" data-type="div"></div>',
        };

        // auto doT templates
        $.each(_factoryData.templateDefinitions, function (key, templateDefinition) {
            _factoryData.templateInstances[key] = doT.template(templateDefinition);
        });


        _factoryData.isInitialized = true;
    }


    // wrap all factory/one-time data in  a single object so we only have to punch one hole in interface to instance code to use
    var _factoryData = {
        isInitialized: false,
        templateDefinitions: {},
        templateInstances: {}
    };

    return _factoryInterface;
}());




// per-instance junk
MessengerCanvasWidgetFactory.createInstance = function (options) {
    var _interface = {   
        sync: function (changeType, state) { },
        handleEvent: function (event) { },
    };

    // ---------------

    // will trigger factory init() if needed
    var _factoryData = this.getFactoryData();
    var _options = $.extend(true, {}, _factoryData.defaultInstanceOptions, options);        
    
   
    var _currentState = {
        canvas: null,
        configDataJson: null
    };


    var _analyticsInfo = {
        appInstanceId: _options.appInstance.id,
        appInstanceObjectId: _options.appInstance.objectId,
        appDefinitionId: _options.appDefinition.id
    };

    function init() {     
        _currentState.configDataJson = _options.appInstance.configurationData;        
        getAppCanvas();
    }

    _interface.sync = function(changeType, state) {        
        switch (changeType) {
            case 'settings':                                    
                //MessengerRuntimeUtilities.syncStyles(_options.runtime.messenger, getTotalWidth(), getTotalHeight());
                //styleFrame('visibility' + (_visible ? 'visible' : 'hidden') + ';  width: ' + getTotalWidth() + 'px; height: ' + getTotalHeight() + 'px; ');
                //setHeight(_options.runtime.messenger.settings.dimensions.messengerHeight);           
                //syncTitleBarContents();
                //syncTitlebarAnimation();
                syncVideoHeights();
                break;
        }        
    }

    _interface.handleEvent = function (event) {
        var id = 'event';
        var values = {
            eventJson: JSON.stringify(event)
        };
        var action = event.type;

        submit(id, values, action);
    }
  

    function drawCanvas() {
        $(_options.container).html(_factoryData.templateInstances.canvas({
            canvas: _currentState.canvas
        }));       

        var canvas = $(_options.container).find('.canvas');
        $.each(_currentState.canvas, function (index, component) {
            drawComponent(canvas, component);
        });

        // add submit handlers
        var onSubmitting = function (e) {
            // determine which element actually has the action on it, might be above
            var element = e.target;
            var id = $(e.target).attr('data-id');            
            var action = $(e.target).attr('data-action');
            while (action == null) {
                element = $(element).parent();
                id = $(element).attr('data-id');
                action = $(element).attr('data-action');
            }

            switch (action) {
                case 'submit':
                case 'initializeSheet':
                    var values = {};

                    var components = $(canvas).find('.canvas_component');
                    $.each(components, function (index, component) {
                        getComponentValue(component, values);
                    });

                    submit(id, values, action);
                    break;
                
                case 'url':
                    var url = $(element).attr('data-action-url');
                    var label = $(element).attr('data-action-url-label');
                    window.open(url, '_blank');

                    // opening in a new tab, should report here
                    sendMessengerEvents([getHydratedEvent({
                        date: new Date().toISOString(),
                        category: 'url',
                        action: url,
                        label: label,
                    })]);
                    break;

                //case 'initializeSheet':
                //    console.log(_options.appDefinition.webhooksUrls);
                //    url = _options.appDefinition.webhooksUrls.initializeSheet;
                //    accessToken = _options.appInstance.accessToken;

                //    _options.callbacks.onOpenSheet({
                //        appInstance: _options.appInstance,
                //        url: _options.appDefinition.webhooksUrls.initializeSheet,
                //        iframeAllow: $(element).attr('data-iframe-allow'),
                //        title: $(element).attr('data-sheet-title')
                //    });
                //    break;
            }
        }
        
        var handlerClickSelectors = [
            '.canvas_button[data-action="submit"]',
            '.canvas_button[data-action="url"]',
            '.canvas_button[data-action="initializeSheet"]',
            
            '.canvas_textbox_container[data-action="submit"] button',
            '.canvas_textbox_container[data-action="url"] button',
            '.canvas_textbox_container[data-action="initializeSheet"] button',

            '.ListItem:not(.disabled)[data-action="submit"]',
            '.ListItem:not(.disabled)[data-action="url"]',
            '.ListItem:not(.disabled)[data-action="initializeSheet"]',
        ];
        $(canvas).find(handlerClickSelectors.join(',')).click(onSubmitting);

        $(canvas).find('[tabindex="0"]').keydown(function(e) {
            if (e.keyCode == 32 || e.keyCode == 13) $(e.target).click();
        });

        var handlerEnterSelectors = [
            '.canvas_textbox_container[data-action="submit"] input',
            '.canvas_textbox_container[data-action="url"] input',
            '.canvas_textbox_container[data-action="initializeSheet"] input',
        ];
        $(canvas).find(handlerEnterSelectors.join(',')).on('keyup', function (e) {
            if (e.keyCode == 13) { // enter key
                onSubmitting(e);
            }
        });

        _options.callbacks.onDrawn();
    }

    function getComponentValue(component, values) {
        var id = $(component).attr('data-id');
        var type = $(component).attr('data-type');

        if (component.components != null && component.components.length > 0) {
            $.each(component.components, function (index, childComponent) {
                getComponentValue(childComponent, values);
            });
        }

        switch (type) {
            case 'textbox':
                values[id] = $(component).find('input').val();
                break;
            case 'dropdown':
                values[id] = $(component).find('option:selected').val();
                break;           
        }
    }

    function syncVideoHeight(container) {
        var aspectRatio = parseFloat($(container).attr('data-aspect-ratio'));
        var width = $(container).width();
        var height = Math.floor(width / aspectRatio);
        $(container).find('iframe').height(height);
    }


    function syncVideoHeights() {
        var containers = $(_options.container).find('.Video');
        $.each(containers, function(index, container) {
            syncVideoHeight(container);
        });
    }


    function drawComponent(uiParent, component) {
        // add component
        var uiComponent = $(_factoryData.templateInstances[component.type](component));
        $(uiParent).append(uiComponent);
        //var uiComponent = $(uiParent).append(_factoryData.templateInstances[component.type](component));

        if (component.components != null && component.components.length > 0) {
            $.each(component.components, function (index, childComponent) {
                drawComponent(uiComponent, childComponent);
            });
        }


        // sync aspect ratio
        if (component.type == 'video') {
            syncVideoHeight(uiComponent);            
        }

        if (component.value != null) {
            //var uiComponent = $(uiParent).find('[data-id="' + component.id + '"]');

            // default value
            switch (component.type) {
                case 'textbox':
                    $(uiComponent).find('input').val(component.value);
                    break;
                case 'dropdown':
                    $(uiComponent).find('select').val(component.value);
                    break;
            }
        }
    }

    function submit(id, values, action) {
        var url = '';
        var accessToken = '';      
        if (action == 'initializeSheet') {
            url = _options.appDefinition.webhooksUrls.initializeSheet;
            accessToken = _options.appInstance.accessToken;
        } else {
            switch (_options.runType) {
                case 'configure':
                    url = _options.appDefinition.webhooksUrls.configure;
                    accessToken = _options.appInstance.adminAccessToken;
                    break;
                case 'initialize':
                    url = _options.appDefinition.webhooksUrls.submit;
                    accessToken = _options.appInstance.accessToken;
                    break;
            }
        }

        $.ajax({
            type: 'POST',
            url: url,
            data: JSON.stringify({
                accessToken: accessToken,
                configDataJson: _currentState.configDataJson,
                submitContextJson: JSON.stringify({
                    id: id,
                    values: values
                })
            }),
            contentType: "application/json; charset=utf-8",
            dataType: 'json',
            success: function (result) {

                // apply response events
                {
                    var events = [];

                    // TODO: are there some we are missing here?
                    if (action != null) {
                        events.push(getHydratedEvent({
                            date: new Date().toISOString(),
                            category: action,
                            action: action,
                            label: action,
                        }));
                    } else {
                        switch (_options.runType) {
                            case 'configure':
                                // do nothing
                                break;
                            case 'initialize':
                                events.push(getHydratedEvent({
                                    date: new Date().toISOString(),
                                    cateogry: 'submit',
                                    action: 'submit',
                                    label: 'Submit',
                                }));
                                break;
                        }
                    }
                
                    if (result.events != null && typeof result.events != 'undefined') {
                        $.each(result.events, function (index, event) {
                            event.custom = true;
                            events.push(getHydratedEvent(event));
                        });
                    }
                
                    if (events.length > 0) {
                        sendMessengerEvents(events);
                    }
                }


                // function to apply response canvas. will get called at different types depending on response "action" for us to take
                var applyCanvasUpdates = function (instance) {
                    // some webhooks calls can return a null canvas which by convention means "keep the canvas you already have"
                    var gotNewCanvas = result.canvas != null && typeof result.canvas != 'undefined';
                    if (gotNewCanvas) _currentState.canvas = result.canvas;         

                    _options.appInstance = instance;
                    _currentState.configDataJson = _options.appInstance.configurationData;

                    if (_currentState.canvas != null && gotNewCanvas) {
                        drawCanvas();
                    } else {
                        _options.callbacks.onClosed();
                    }
                };                

                // apply response action, if any
                {
                    var actionToApplyType = null;
                    if (result.action != null && typeof result.action != 'undefined') actionToApplyType = result.action.type;                    
                    var actionToApply = result.action;

                    switch (actionToApplyType) {
                        // app telling us to open a sheet
                        case 'sheet':
                            {
                                var onSheetOpenedFn = function() {
                                    applyCanvasUpdates(_options.appInstance);
                                };
                                _options.callbacks.onOpenSheet({
                                    appInstance: _options.appInstance,
                                    url: actionToApply.context.url,
                                    iframeAllow: actionToApply.context.iframeAllow,
                                    title: actionToApply.context.title,
                                    metaData: actionToApply.context.metaData
                                }, onSheetOpenedFn); 
                            }
                            break;

                        //case 'url': // future use
                            //break;

                        // app telling us about updated data (e.g. as result of "config" call):
                        case 'update':
                            if (actionToApply.context.configDataJson != null) {
                                // update, fire an update event to parent who will pick it up and save app instance to DB.  assume this works, and proceed to onSaveDone
                                _options.appInstance.configurationData = actionToApply.context.configDataJson;                                                    
                                sendEvent({
                                    type: 'APP_INSTANCE_UPDATED',
                                    appInstance: _options.appInstance
                                });                                
                            }
                            applyCanvasUpdates(_options.appInstance);
                            break;
                    
                        // no action/action we dont understand: just apply canvas updates
                        default:
                            applyCanvasUpdates(_options.appInstance);
                            break;
                    }
                }
            },
        });
    }

    function getHydratedEvent(event) {
        var hydrated = $.extend(true, {}, event, _analyticsInfo);

        // set the category to the app name if it is not filled in by the server
        if (hydrated.category == null || hydrated.category.length < 1) {
            hydrated.category = _options.appDefinition.name;
        }

        return hydrated;
    }

    function getAppCanvas()  {
        var url = '';
        var accessToken = '';
        switch (_options.runType) {
            case 'configure':
                url = _options.appDefinition.webhooksUrls.configure;
                accessToken = _options.appInstance.adminAccessToken;
                break;
            case 'initialize':
                url = _options.appDefinition.webhooksUrls.initialize;
                accessToken = _options.appInstance.accessToken;
                break;
        }

        $.ajax({            
            type: 'POST',
            url: url,
            data: JSON.stringify({
                accessToken: accessToken,
                configDataJson: _currentState.configDataJson,                    
                submitContextJson: null
            }),
            contentType: "application/json; charset=utf-8",
            dataType: 'json',
            success: function (result) {
                var events = [];

                // generate events
                switch (_options.runType) {
                    case 'configure':
                        // do nothing
                        break;
                    case 'initialize':
                        events.push(getHydratedEvent({
                            date: new Date().toISOString(),
                            category: 'initialize',
                            action: 'initialize',
                            label: 'Initialize',
                        }));

                        _options.callbacks.onInitialized();
                        break;
                }

                if (result.events != null) {
                    $.each(result.events, function (index, event) {
                        event.custom = true;
                        events.push(getHydratedEvent(event));
                    });
                }

                if (events.length > 0) {
                    sendMessengerEvents(events);
                }

                _currentState.canvas = result.canvas;
                drawCanvas();
            },
        });
    }

    function sendMessengerEvents(events) {
        $(_options.eventContainer).trigger({
            type: 'messengerEvents',
            context: {
                events: events
            }
        });
    }

    function sendEvent(message) {
        window.parent.postMessage(message, '*');
    }

    init();
    return _interface;
};
// one-time junk
var MessengerLauncherWidgetFactory = (function () {
    var _factoryInterface = {
        createInstance: function (options) { return null; },
        getFactoryData: function () { if (!_factoryData.isInitialized) { init(); } return _factoryData; },        

        //getIframeMessageBus: function() {
        //    var bus = {
        //        publish: function(event) {},
        //        subscribe: function(id) {},
        //        unsubscribe: function(id) {}
        //    };
        //}
    };

    // initialize factory/one-time data.  done only when first instance is created
    function init() {
        _factoryData.defaultInstanceOptions = {
            container: null,
            eventContainer: null,
            token: null,
            
            runtime: null,
            messengerContext: null,

            height: 200,
            showClose: false,
            realtimeHubsBase: null,
            makeContainersTransparent: true, // false when running in test mode
            messageBus: null,
            callbacks: {
                onClose: function () { },
                onMessengerContextChanged: function(messengerContext) {}
            }
        };

        _factoryData.templateDefinitions = {
            control:                
                '<div class="MessengerLauncher" style="height:{{=it.context.viewState.launcherButton.height}}px; width:{{=it.context.viewState.launcherButton.width}}px;">' +
                    '<div class="CircleButton Button" tabindex="0">' +
                        '<span class="ButtonContent" tabindex="-1">' +
                            '<i data-mode="MINIMIZED" class="si-close"></i>' +
                            '<i data-mode="MAXIMIZED" class="si-messages"></i>' +
                        '</span>' +
                    '</div>' +
                '</div>'
        };

        // auto doT templates
        $.each(_factoryData.templateDefinitions, function (key, templateDefinition) {
            _factoryData.templateInstances[key] = doT.template(templateDefinition);
        });


        _factoryData.isInitialized = true;
    }


    // wrap all factory/one-time data in  a single object so we only have to punch one hole in interface to instance code to use
    var _factoryData = {
        isInitialized: false,
        templateDefinitions: {},
        templateInstances: {}
    };

    return _factoryInterface;
}());




// per-instance junk
MessengerLauncherWidgetFactory.createInstance = function (options) {
    var _interface = {
        destroy: function() {},
        sync: function(changeType, runtime) {}
    };

    // ---------------

    // will trigger factory init() if needed
    var _factoryData = this.getFactoryData();
    var _options = $.extend(true, {}, _factoryData.defaultInstanceOptions, options);    

    var _panelWidget = null;
    var _launcherAnimating = false;
    var _lastFrameStyles = '';
    
    function ready() {
        // send a message once frame is loaded and ready to work.  init() gets called afterwards
        window.addEventListener('message', handleEvent, false);

        sendEvent({
            type: 'FRAME_READY',
            frameType: 'Launcher'
        });
    }


    function init() {                        
        if (_options.eventContainer == null) _options.eventContainer = _options.container;

        // make all parent elements have transparent background 
        if (_options.makeContainersTransparent) {
            $(_options.container).parents().css('background', 'transparent');
        }

        // initialize view state if it doesnt exist
        _options.context = { viewState: null };
        if (_options.context.viewState == null) {
            _options.context.viewState = {
                view: _options.viewOptions.defaultView,
                launcherButton: {
                    width: 75,
                    height: 75,
                    marginTop: 20
                },
                panel: {                    
                    width: 300,
                    height: 500
                }
            };
        }
        
        MessengerRuntimeUtilities.syncStyles(_options.runtime.messenger, getTotalWidth(), getTotalHeight());

        draw();

        setView(_options.context.viewState.view, true);
    }

    _interface.destroy = function() {
    };


    _interface.sync = function(changeType, state) {
        _options.runtime = state.runtime;
        _options.messengerContext = state.messengerContext;        
        styleFrame(_lastFrameStyles);
        
        switch (changeType) {
            //case 'layout':
            //    if (_panelWidget != null) _panelWidget.sync(changeType, state);
            //    break;

            case 'settings':          
                MessengerRuntimeUtilities.syncStyles(_options.runtime.messenger, getTotalWidth(), getTotalHeight());
                syncLauncherUi();
                if (_panelWidget != null) _panelWidget.sync(changeType, state);
                break;
        }
        
    }

    function syncLauncherUi(){
        var launcher = _options.runtime.messenger.settings.launcher;
        var maximizeElement = $(_options.container).find('.MessengerLauncher [data-mode="MAXIMIZED"]');
        var minimizeElement = $(_options.container).find('.MessengerLauncher [data-mode="MINIMIZED"]');

        if (launcher.useCustomShowUrl && launcher.customShowUrl != ""){
            var html = '<img src="' + launcher.customShowUrl + '" alt="Maximize button">'
            maximizeElement.removeClass('si-messages').html(html);
        }else if(!maximizeElement.hasClass('si-messages')){
            maximizeElement.addClass('si-messages').html("");
        }
        if (launcher.useCustomHideUrl && launcher.customHideUrl != ""){
            var html = '<img src="' + launcher.customHideUrl + '" alt="Maximize button">'
            minimizeElement.removeClass('si-close').html(html);
        }else if (!minimizeElement.hasClass('si-close')){
            minimizeElement.addClass('si-close').html("");
        }
    }


    function styleFrame(styles) {        
        var messengerSettings = _options.runtime.messenger.settings;

        var baseStyles = 'position: fixed;';
        if (messengerSettings.general.messengerPosition == "right") baseStyles += ' right: ' + messengerSettings.dimensions.horizontalOffset + 'px;';
        if (messengerSettings.general.messengerPosition == "left") baseStyles += ' left: ' + messengerSettings.dimensions.horizontalOffset + 'px;';
        baseStyles += ' bottom: ' + messengerSettings.dimensions.verticalOffset + 'px;';

        sendEvent({
            type: 'STYLE_FRAME_CONTAINER',
            frameType: 'Launcher',
            styles: baseStyles + styles
        });

        _lastFrameStyles = styles;
    }

    // emit this event to "server"/launcher JS, rather than applying it immediately.  application happens when "server"/launcher broadcasts event back to us.
    // why bother with this? because if the *panel* ever wanted to set the visibility (e.g. if we added a "close" button to messenger panel), then all it would have to
    // do is emit this same event and things would just work.  this pattern of piping execution flow through the server could/should probably be formalized/automated
    function setMessengerPanelVisibilityEmit(visible, isInitialDrawing) {
        sendEvent({
            type: 'SET_PANEL_VISIBILITY',
            visible: visible,
            isInitialDrawing: isInitialDrawing
        });
    }

    
    function setMessengerPanelVisibility(visible, isInitialDrawing) {
        var viewState = _options.context.viewState;
        var animationSpeed = MessengerRuntimeUtilities.getAnimationSpeed(_options.runtime.messenger.settings.general.animationSpeed);
        if (isInitialDrawing) animationSpeed = 0;
        
        
        if (animationSpeed > 0) _launcherAnimating = true;

        if (visible) {            
            $(_options.container).find('.MessengerLauncher [data-mode="MAXIMIZED"]')
                .css('transform', 'scale(0)')
                .css('transition',  animationSpeed + 's');

            $(_options.container).find('.MessengerLauncher [data-mode="MINIMIZED"]')
                .css('transform', 'scale(1)')
                .css('transition',  animationSpeed + 's');

        } else {
            $(_options.container).find('.MessengerLauncher [data-mode="MAXIMIZED"]')
                .css('transform', 'scale(1)')
                .css('transition',  animationSpeed + 's');

            $(_options.container).find('.MessengerLauncher [data-mode="MINIMIZED"]')
                .css('transform', 'scale(0)')
                .css('transition',  animationSpeed + 's');
        }
    }


    function setView(view, isInitialDrawing) {
        var views = MessengerRuntimeUtilities.getViews(); 

        var viewState = _options.context.viewState;

        switch (view) {
            case views.hidden.type:
                break;

            case views.minimized.type:
                {
                    // show + size launcher button     
                    styleFrame('visibility: visible;  width: ' + getTotalWidth() + 'px; height: ' + getTotalHeight() + 'px; ');

                    // hide messenger panel
                    setMessengerPanelVisibilityEmit(false, isInitialDrawing);
                    $(_options.container).find('.Button').attr('aria-label', 'Maximize Messenger');
                }
                break;

            case views.maximized.type:
                styleFrame('visibility: visible;  width: ' + getTotalWidth() + 'px; height: ' + getTotalHeight() + 'px; ');
                setMessengerPanelVisibilityEmit(true, isInitialDrawing);             
                $(_options.container).find('.Button').attr('aria-label', 'Minimize Messenger');
                break;
        }

        sendEvent({
            type: view,
            frameType: 'Launcher'
        });
    }


    function handleEvent(message) {
        if (message.data == null) return;
        
        var event = message.data;        
        if (event.type == null) return;

        
        switch (event.type) {
            case 'ALL_FRAMES_READY':
                _options.viewOptions = event.settings.viewOptions;
                init();
                break;

            case 'SYNC':
                _interface.sync(event.changeType, event.state);
                break;

            case 'SET_PANEL_VISIBILITY':                
                setMessengerPanelVisibility(event.visible, event.isInitialDrawing);
                break;

            case 'MINIMIZE':
                onLauncherClicked();
                break;

            case 'ATTENTION_STARTED':
                onAttentionStarted();
                break;
        }
    }


    function sendEvent(message) {
        window.parent.postMessage(message, '*');
    }


    function onLauncherClicked() {
        var viewState = _options.context.viewState;
        var views = MessengerRuntimeUtilities.getViews();
        
        if (viewState.view == views.minimized.type) {
            viewState.view = views.maximized.type;
        } else {
            viewState.view = views.minimized.type;
        }
        setView(viewState.view);
    }

    function onAttentionStarted() {
        var toSpin = $(_options.container).find('.ButtonContent > i[data-mode="MINIMIZED"]');

        $(toSpin).css({
            '-webkit-animation-name': 'spin',
            '-webkit-animation-duration': '1s',
            '-webkit-animation-iteration-count': '1',
            '-webkit-animation-timing-function': 'linear',
            '-moz-animation-name': 'spin',
            '-moz-animation-duration': '1s',
            '-moz-animation-iteration-count': '1',
            '-moz-animation-timing-function': 'linear',
            '-ms-animation-name': 'spin',
            '-ms-animation-duration': '1s',
            '-ms-animation-iteration-count': '1',
            '-ms-animation-timing-function': 'linear',
        });

        setTimeout(function() {
            $(toSpin).css({
                '-webkit-animation-name': '',
                '-webkit-animation-duration': '',
                '-webkit-animation-iteration-count': '',
                '-webkit-animation-timing-function': '',
                '-moz-animation-name': '',
                '-moz-animation-duration': '',
                '-moz-animation-iteration-count': '',
                '-moz-animation-timing-function': '',
                '-ms-animation-name': '',
                '-ms-animation-duration': '',
                '-ms-animation-iteration-count': '',
                '-ms-animation-timing-function': '',
            });
        }, 1100);
    }

    function draw() {
        _options.totalHeight = getTotalHeight();
        _options.totalWidth = getTotalWidth();
        $(_options.container).html(_factoryData.templateInstances.control(_options));

        $(options.container).find('.MessengerLauncher [data-mode="MAXIMIZED"], .MessengerLauncher [data-mode="MINIMIZED"]')
        .on('transitionend', function(e) {
            _launcherAnimating = false;
        });

        //$(_options.container).find('.MessengerLauncher .ButtonContent')
        $(_options.container).find('.MessengerLauncher .Button')
            .click(function(e) {
                if (!_launcherAnimating) onLauncherClicked();
            })        
            .keydown(function(e) {
                if (e.keyCode == 32 || e.keyCode == 13) $(e.target).click();
            });
        syncLauncherUi();

    }      
    
    function getTotalHeight() {
        var viewState = _options.context.viewState;
        var padding = 0;
        var t = 
            viewState.launcherButton.height +
            padding;
        return t;
    }


    function getTotalWidth() {
        var viewState = _options.context.viewState;        
        var t = 
            viewState.launcherButton.height;
        return t;
    }

    ready();
    return _interface;
};
// one-time junk
var MessengerRuntimeWidgetFactory = (function () {
    var _factoryInterface = {
        createInstance: function (options) { return null; },
        getFactoryData: function () { if (!_factoryData.isInitialized) { init(); } return _factoryData; },

        getEvents: function () {
            return {
                styleFrame: 'STYLE_FRAME'
            };
        },
        getAnimationSpeed(animationSpeed) {
            var speeds = {
                'none': 0,
                'fast': 0.2,
                'medium': 0.4,
                'slow': 1.0
            };

            try {
                var speed = speeds[animationSpeed.toLowerCase()];
                if (typeof speed != 'undefined') return speed;
            } catch (e) {}
            return speeds['medium'];
        }        
    };

    // initialize factory/one-time data.  done only when first instance is created
    function init() {
        _factoryData.defaultInstanceOptions = {
            container: null,
            eventContainer: null,
            token: null,
            context: null,            
            height: 200,
            showClose: false,
            realtimeHubsBase: null,
            makeContainersTransparent: true, // false when running in test mode
            callbacks: {
                onClose: function () { },
                onMessengerContextChanged: function(messengerContext) {}
            }
        };

        _factoryData.templateDefinitions = {
            control:
                '<div class="MessengerRuntime">' +
                    '<div class="PanelContainer">' +          
                    '</div>' +


                    '<div class="Launcher" style="height:{{=it.context.viewState.launcherButton.height}}px; width:{{=it.context.viewState.launcherButton.width}}px; margin-top:{{=it.context.viewState.launcherButton.marginTop}}px;">' +
                        '<div class="CircleButton Button">' +
                            '<i data-mode="MAXIMIZED" class="si-close"></i>' +
                            '<i data-mode="MINIMIZED" class="si-messages"></i>' +
                        '</div>' +
                    '</div>' +

                    //'<table class="main_table">' +
                    //    '<tr>' +
                    //        '{{?it.mode == "workspace"}}' +
                    //        '<td class="sidebar_cell">' +
                    //             // sidebar
                    //            '<div class="agent_sidebar_container"></div>' +
                    //        '</td>' +
                    //        '{{?}}' +
                    //        '<td class="view_cell" width=100%>' +
                    //            '<div class="activeViewContainer" style="height: {{=it.height}}px;"></div>' + 
                    //        '</td>' +
                    //    '</tr>' +
                    //'</table>' +
                    //'<div class="error_overlay" style="height: {{=it.height}}px; display: none;">' +
                    //    '' +
                    //'</div>' +
                '</div>',

            minimized:
                '<div class="MinimizedMode">' +
                    '<div class="CircleButton">' +
                        '<i class="si-messages"></i>' +
                    '</div>' +
                '</div>' 
        };

        // auto doT templates
        $.each(_factoryData.templateDefinitions, function (key, templateDefinition) {
            _factoryData.templateInstances[key] = doT.template(templateDefinition);
        });


        _factoryData.isInitialized = true;
    }


    // wrap all factory/one-time data in  a single object so we only have to punch one hole in interface to instance code to use
    var _factoryData = {
        isInitialized: false,
        templateDefinitions: {},
        templateInstances: {}
    };

    return _factoryInterface;
}());





// per-instance junk
MessengerRuntimeWidgetFactory.createInstance = function (options) {
    var _interface = {
        destroy: function() {},
        sync: function(changeType, runtime) {}
    };

    var _api = null;
    var _agentApi = null;

    // ---------------

    // will trigger factory init() if needed
    var _factoryData = this.getFactoryData();
    var _options = $.extend(true, {}, _factoryData.defaultInstanceOptions, options);    

    var _panelWidget = null;

    //var _viewState = {
    //    selectedView: MessengerRuntimeWidgetFactory.getViews().minimized.type,        
    //    allowDragging: true,
    //    allowResizing: true,
    //    windowAnchor: {        
    //        x: AgentWorkspaceWidgetFactory.getSnapStates().x.left,
    //        y: AgentWorkspaceWidgetFactory.getSnapStates().y.bottom             
    //    }
    //};


    function validateRuntime() {
        if (_options.context.runtime.messenger.appLayout == null) {
            _options.context.runtime.messenger.appLayout = {
                panelList: []
            };
        }
    }


    function init() {                        
        validateRuntime();

        if (_options.eventContainer == null) _options.eventContainer = _options.container;

        // make all parent elements have transparent background 
        if (_options.makeContainersTransparent) {
            $(_options.container).parents().css('background', 'transparent');
        }

        // set up the api to use the agent set token for authorize
        _api = MakeAstuteSocialAPI({ accessToken: _options.token, endpoint: null});                
        
        // initialize view state if it doesnt exist
        if (_options.context.viewState == null) {
            _options.context.viewState = {
                view: _options.context.viewOptions.defaultView,
                launcherButton: {
                    width: 75,
                    height: 75,
                    marginTop: 20
                },
                panel: {                    
                    width: 300,
                    height: 500
                }
            };
        }
        
        syncStyles();

        draw();

        setView(_options.context.viewState.view, true);

        //$(_options.eventContainer).on('data.model.update', onModelDataChanged);

        window.addEventListener('message', handleEvent, false);        

        // send a message once everything
        //sendEvent({
            //type: AgentWorkspaceWidgetFactory.getEvents().workspaceReady,
        //});

        //draw();
    }

    _interface.destroy = function() {
        //if (_sideBarWidget != null) {
        //    _sideBarWidget.destroy();
        //    _sideBarWidget = null;
        //}
        //if (_selectedViewWidget != null) {
        //    _selectedViewWidget.destroy();
        //    _selectedViewWidget = null;
        //}

        if (_panelWidget != null) {
            _panelWidget.destroy();
            _panelWidget = null;
        }

        //$(_options.eventContainer).off('data.model.update', onModelDataChanged);
    };



    _interface.sync = function(changeType, state) {
        _options.context.runtime = state.runtime;
        _options.context.context = state.messengerContext;     
        validateRuntime();
        
        switch (changeType) {
            case 'layout':
                if (_panelWidget != null) _panelWidget.sync(changeType, state);
                break;

            case 'settings':          
                syncStyles();
                if (_panelWidget != null) _panelWidget.sync(changeType, state);
                break;
        }
        
    }


    


    function syncStyles() {        
        var dimensions = _options.context.runtime.messenger.settings.dimensions;
        var colors = _options.context.runtime.messenger.settings.colors;

        var messengerHeight = dimensions.messengerHeight;
        var titleHeight = dimensions.titleHeight;
        var sheetTitleHeight = dimensions.sheetTitleHeight;
        var sheetFrameHeight = messengerHeight - sheetTitleHeight;
        var messengerBorderRadius = dimensions.messengerBorderRadius;

        // remove eventually
        if (typeof messengerHeight == 'string') messengerHeight = parseInt(messengerHeight);
        if (typeof titleHeight == 'string') titleHeight = parseInt(titleHeight);

        // generate style sheet from messenger.settings:
        var presetStyles =
            // dimensions
            '.MessengerRuntime { height: ' + getTotalHeight() + 'px; width: ' + getTotalWidth() + 'px; }\n' +

            '.MessengerPanel { height: ' + messengerHeight + 'px; }\n' +
            '.MessengerPanel .nano { height: ' + messengerHeight + 'px; }\n' +
            '.Sheet { height: ' + messengerHeight + 'px; }\n' +

            '.MessengerPanel .Main .Header { height: ' + (titleHeight - 0) + 'px; }\n' +
            '.MessengerPanel .Main .App[data-index="0"] { margin-top: ' + (titleHeight + 10) + 'px; }\n' +
            '.MessengerPanel .Sheet .Header { height: ' + (sheetTitleHeight - 0) + 'px; }\n' +
            '.MessengerPanel .Sheet .Frame { height: ' + (sheetFrameHeight - 0) + 'px; }\n' +

            '.MessengerPanel { border-radius: ' + messengerBorderRadius + 'px; }\n' +

            // colors
            '.Launcher .CircleButton, .MessengerPanel .Header { background: ' + colors.primary + '}\n' +
            '.MessengerPanel .App { border-top-color: ' + adjustColor(colors.primary, .25) + '; background: ' + colors.background + '; }\n' +

            '.MessengerPanel .Header .Title { color: ' + colors.primaryTitle + '; }\n' +
            '.MessengerPanel .Header .Subtitle { color: ' + colors.primarySubtitle + '; }\n' +
            '.MessengerPanel .Header .CloseSheet { color: ' + colors.primaryTitle + '; }\n' +

            '.MessengerPanel, .MessengerPanel .Sheet { background: ' + colors.background + '; }\n' +

            // primary button colors
            '.MessengerPanel .App .btn-primary, .MessengerPanel .App .btn-primary:focus { background-color: ' + adjustColor(colors.primary, -.25) + '; border-color: ' + adjustColor(colors.primary, -.5) + '; color: ' + colors.primaryTitle + ';}\n' +
            '.MessengerPanel .App .btn-primary:hover, .MessengerPanel .App .btn-primary:active { background-color: ' + adjustColor(colors.primary, .25) + '; }\n' +

            //'.MessengerPanel .App { border-top-color: ' + colors.primary + '}\n' +
            

            //$(_options.container).find('.Header').css('background', _options.colors.primary);
            //$(_options.container).find('.App').css('background', _options.colors.appBackground);
            //$(_options.container).find('.App').css('border-top-color', _options.colors.primary);            

            '';


            //'.MessengerPanel .Main .Header


        // todo: append customCss so presets are overridable


        // add css to page
        var html = '<style type="text/css" class="MessengerRuntimeStyles">' + presetStyles + '</style>';

        var existing = $('head').find('style.MessengerRuntimeStyles');
        if (existing.length > 0) {
            $(existing).replaceWith(html);
        } else {
            $('head').append(html);
        }
    }


    function styleFrame(styles) {
        var viewState = _options.context.viewState;
        var baseStyles = 'position: absolute; right: 20px; bottom: 20px; ';

        sendEvent({
            type: MessengerRuntimeWidgetFactory.getEvents().styleFrame,                        
            styles: baseStyles + styles
        });
    }

    function setMessengerPanelVisibility(visible, isInitialDrawing) {
        // todo: animation
        var viewState = _options.context.viewState;
        var animationSpeed = MessengerRuntimeWidgetFactory.getAnimationSpeed(_options.context.runtime.messenger.settings.general.animationSpeed);
        if (isInitialDrawing) animationSpeed = 0;

        if (visible) {
            $(_options.container).find('.PanelContainer').fadeIn(animationSpeed * 1000);
            
            $(_options.container).find('.Launcher i[data-mode="MAXIMIZED"]')
                .css('transform', 'scale(1)')
                .css('transition',  animationSpeed + 's');

            $(_options.container).find('.Launcher i[data-mode="MINIMIZED"]')
                .css('transform', 'scale(0)')
                .css('transition',  animationSpeed + 's');

        } else {
            $(_options.container).find('.PanelContainer').fadeOut(animationSpeed * 1000)

            $(_options.container).find('.Launcher i[data-mode="MAXIMIZED"]')
                .css('transform', 'scale(0)')
                .css('transition',  animationSpeed + 's');

            $(_options.container).find('.Launcher i[data-mode="MINIMIZED"]')
                .css('transform', 'scale(1)')
                .css('transition',  animationSpeed + 's');
        }

        if (visible) {
            if (_panelWidget == null) {
                _panelWidget = MessengerPanelWidgetFactory.createInstance({
                    container: $(_options.container).find('.PanelContainer'),
                    height: _options.context.runtime.messenger.settings.dimensions.messengerHeight,//  viewState.panel.height,
                    runtime: _options.context.runtime,
                    messengerContext: _options.context.context
                });
            }
        }
    }


    function setView(view, isInitialDrawing) {
        var views = MessengerRuntimeWidgetFactory.getViews(); 

        var viewState = _options.context.viewState;

        switch (view) {
            case views.hidden.type:
                break;

            case views.minimized.type:
                {
                    // show + size launcher button
                    //$(_options.container).find('.Launcher').height(viewState.launcherButton.height);
                    //$(_options.container).find('.Launcher').width(viewState.launcherButton.width);                    
                    styleFrame('visibility: visible;  width: ' + getTotalWidth() + 'px; height: ' + getTotalHeight() + 'px; ');

                    // hide messenger panel
                    setMessengerPanelVisibility(false, isInitialDrawing);
                }
                break;

            case views.maximized.type:
                styleFrame('visibility: visible;  width: ' + getTotalWidth() + 'px; height: ' + getTotalHeight() + 'px; ');
                setMessengerPanelVisibility(true, isInitialDrawing);                
                break;
        }
    }

    function onModelDataChanged(event) {
