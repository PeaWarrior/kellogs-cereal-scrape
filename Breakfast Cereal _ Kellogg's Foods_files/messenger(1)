    function mapDataChanged(workspaceEvent) {
        var dataChangedMap = AgentWorkspaceWidgetFactory.getDataChanged();

        var dataChanged = null;
        switch (workspaceEvent.objectType) {
            case 0:
                dataChanged = dataChangedMap.agentSet;
                break;
            case 1:
                dataChanged = dataChangedMap.agent;
                break;
            case 2:
                dataChanged = dataChangedMap.agentQueue;
                break;
            case 3:
                dataChanged = dataChangedMap.agentQueueRing;
                break;
            case 4:
                dataChanged = dataChangedMap.agentConversation;
                break;
            case 5:
                dataChanged = dataChangedMap.skill;
            default:
                break;
        }

        return dataChanged;
    }

    //function handleAddedOrUpdatedEvent(workspaceEvent) {
